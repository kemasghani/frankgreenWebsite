script>\n        document.addEventListener('scroll', function () {\n            const productImage = document.querySelector('.custom2-product-image');\n            const productSection = document.querySelector('.custom2-product-section');\n            const description = document.querySelector('.custom2-description');\n\n            const sectionHeight = productSection.offsetHeight;\n            const scrollY = window.scrollY;\n            const shrinkStart = productSection.offsetTop; \n            const shrinkEnd = shrinkStart + sectionHeight * 0.4; \n            if (scrollY > shrinkStart && scrollY <= shrinkEnd) {\n                const progress = (scrollY - shrinkStart) / (shrinkEnd - shrinkStart);\n\n                const scale = 1 - progress * 0.75;\n                productImage.style.transform = `scale(${scale})`;\n\n                const translateY = progress * 100; \n                productImage.style.transform += ` translateY(${translateY}%)`;\n\n                const translateYText = -50 + progress * 50;\n                description.style.transform = `translateY(${translateYText}%)`;\n            } else if (scrollY > shrinkEnd) {\n                productImage.style.transform = 'scale(0.25) translateY(100%)';\n\n                description.style.transform = `translateY(0%)`;\n            } else {\n                productImage.style.transform = 'scale(1) translateY(0%)';\n\n                description.style.transform = `translateY(-50%)`;\n            }\n        });\n    </script>\n</body>\n</html>",
